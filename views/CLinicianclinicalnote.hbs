<div id="sidenav" class="sidenav">
    <img src="/img/Group-44.png" />
    <p onClick="location.href='/clinicians/home'">{{doctor.firstName}}</p >

    <img src="/img/icons8-dashboard-64.png" />
    <p onClick="location.href='/clinicians/dashboard'">Dashboard</p >

    <img src="/img/Comment-1.png" />
    <p onClick="location.href='/clinicians/checkComment'">Comment</p >

    <img src="/img/icons8-add-user-male-96.png" />
    <p onClick="location.href='/clinicians/createProfile'">Create</p >

    <img src="/img/icons8-search-account-64.png" />
    <p onClick="location.href='/clinicians/profile'">Profile</p >
    <form method="POST" action="/clinicians/logout">
    <button>Log out</button>
    </form>
</div>
<form action="/clinicians/dashboard/{{patient._id}}/listClinicalNotes" method="GET" class="clin_note_form" style="margin-top: 100px;">
<div class="ClinicalNote"> 
    Clinical Notes
</div>
<div class="clinical_box2">
    <div >Patient Name: </div>
    <div class="clinical_box">{{patient.firstName}}</div>
    <div > </div>
</div>
<ul>
{{#each notes}}
{{!-- <div class="two_a"> --}}
    <li class="list_of_notes"><div><span class="list_of_notes_date"> {{recordDate}}</span><div class="list_of_notes_detail">{{this.text}}</div></div></li>
{{!-- </div>         --}}
{{/each}}
</ul>
<button class="send" style="margin-left: 200px;border:0;"><a href="/clinicians/dashboard/{{patient._id}}/clinicalNotes">Add New</a></button>
</form>
