<form action="/patients/rank" method="GET"></form>
<section>
  <div class="ranking-box">
    <div class="box-title">Top 5 Badge Ranking</div>
    <div class="box-tip">You Have Been Here For <span class="red">{{day}}</span> Days</div>

  <div style="display: flex;justify-content: center;">
    <div class="rank">
      <div class="">
        <div class="rank-thead">
          <div class="rank-title">Rank</div>
          <div class="rank-title">Name</div>
          <div class="rank-title">Rate</div>
          <div class="rank-title">Badge</div>
        </div>
        <div class="ranl_cont_a">


          <div>
            <div class="">No.1</div>
            <div class="">No.2</div>
            <div class="">No.3</div>
            <div class="">No.4</div>
            <div class="">No.5</div>
          </div>
          <div>
            {{#each rank}}
            <div class="rank_tilimg">

                <img style="width: 40px;height:40px" src="../public/img/head.png" alt="">

              <div style="text-align: center;">
                <span>{{this.userName}}</span>
              </div>

            </div>
            {{!-- <div class="">{{this.recordRate}}</div> --}}
            {{/each}}
          </div>

          <div>
            {{#each rank}}

            <div class="">{{this.recordRate}}</div>
            {{/each}}
          </div>

          <div class="right_mg">
            <div class=""><img src="../public/img/No.1.png" name="" alt=""></div>
            <div class=""><img src="../public/img/No.2.png" name="" alt=""></div>
            <div class=""><img src="../public/img/No.3.png" name="" alt=""></div>
            <div class=""><img src="../public/img/badge.png" name="" alt=""></div>
            <div class=""><img src="../public/img/badge.png" name="" alt=""></div>
          </div>






        </div>
      </div>
    </div>
  </div>


  </div>
</section>
</form>