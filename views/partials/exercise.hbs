<div id='Exercise' style="width: 100%;height:400px;margin-top:20px"></div>
<script src="https://cdn.bootcdn.net/ajax/libs/echarts/5.3.2/echarts.common.js"></script>
<script type='text/javascript' >
var myChart2 = echarts.init(document.getElementById('Exercise'))
var option2 =  {
    title: {
    left: 'center',
    text: 'Excercise'
  },
  tooltip: {
    trigger: 'axis',
    axisPointer: {
      type: 'cross',
      label: {
        backgroundColor: '#6a7985'
      }
    }
  },
  xAxis: {
    type: 'category',
    data: {{{dates}}}
  },
  yAxis: {
    type: 'value'
  },
  series: [
    {
      data: {{{datas}}}.exercise,
      type: 'line'
    }
  ]
};

option2 && myChart2.setOption(option2);
</script>