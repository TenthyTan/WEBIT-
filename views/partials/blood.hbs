<div id='Blood' style="width: 100%;height:400px;margin-top:20px"></div>
<script src="https://cdn.bootcdn.net/ajax/libs/echarts/5.3.2/echarts.common.js"></script>
<script type='text/javascript' >
var myChart1 = echarts.init(document.getElementById('Blood'))
var option1 =  {
    title: {
    left: 'center',
    text: 'Blood Glucose Level (nmol/L)'
  },
  tooltip: {
    trigger: 'axis',
    axisPointer: {
      type: 'cross',
      label: {
        backgroundColor: '#6a7985'
      }
    }
  },
  xAxis: {
    type: 'category',
    data: {{{dates}}}
  },
  yAxis: {
    type: 'value'
  },
  series: [
    {
      data: {{{datas}}}.bgl,
      type: 'line'
    }
  ]
};

option1 && myChart1.setOption(option1);
</script>